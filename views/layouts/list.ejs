<section id="main">
      <div class="container">
        <div class="row">
            <a class="btn btn-primary" href="../blogs/add">Create</a>
            <table class="table table-striped">
                <tr>
                    <th>
                        ID
                    </th>
                    <th>
                        Topic
                    </th>
                    <th>
                        Content
                    </th>
                    <th>
                        Action
                    </th>
                </tr>
                <% objs.forEach(function(row){ %>
                <tr>
                    <td>
                        <%= row.id %>
                    </td>
                    <td>
                        <%= row.topic %>
                    </td>
                    <td>
                        <%= row.content %>
                    </td>
                    <td>
                        <a id="edit" class="fl btn btn-primary" href="../admin/blogs/edit/<%=row.id%>">Edit</a>
                        <form action="../admin/blogs/delete/<%=row.id%>" method="post" onsubmit="return(confirm('Do you want to delete this ?'))">
                        <button type="submit" class="btn btn-danger">Delete</button>
                        </form>
                    </td>
                </tr>
                <% }); %>
            </table>
        </div>
      </div>
</section>
