<section id="main">
      <div class="container">
        <div class="row">
            <a class="btn btn-primary" href="../blogs/add">Create</a>
            <table class="table table-striped">
                <tr>
                    <th>
                        NO.
                    </th>
                    <th>
                        ID
                    </th>
                    <th>
                        Topic
                    </th>
                    <th>
                        Content
                    </th>
                    <th>
                        Action
                    </th>
                </tr>
                <% for(var i = 0;i < data.length;i++) { %>
                <tr>
                    <td><%=(i+1)%></td>
                    <td>
                        <%= data[i].id %>
                    </td>
                    
                </tr>
                <% } %>
            </table>
        </div>
      </div>
</section>
